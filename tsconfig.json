{
"compilerOptions": {
    "module": "commonjs",
    "lib": ["es6", "dom"],
    "esModuleInterop" : false,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "noUnusedLocals": false,
    "moduleResolution": "node",
    "outDir": "helenaCuteCute",
    "sourceMap": false,
    "strict": false,
    "target": "es5"
  },
  "compileOnSave": true,
  "include": [
    "helenaCute/**/*"
  ]
}
